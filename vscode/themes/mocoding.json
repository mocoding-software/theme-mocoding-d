{
  "name": "Mocoding-D",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#E8E0C8",
    "badge.background": "#569CD6",
    "editorCursor.foreground": "#E8E0C8"
  },
  "tokenColors": [
     {
      "name": "Other Text",
      "scope": "variable, meta.object, meta.tag",
      "settings": {        
        "foreground": "#E8E0C8"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword.control, keyword.type, keyword.other, keyword.operator.new, keyword.operator.expression, constant.language, storage, primitive, modifier, language.constant, meta.preprocessor, variable.language.this, support.type.primitive",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF8811"
      }
    },    
    {
      "name": "Operator / Punctuation",
      "scope": "punctuation, keyword.operator, storage.type.function.arrow",
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Type Parameters",
      "scope": "meta.type.parameters, entity.name.type.type-parameter",
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "entity.name.type.alias, entity.name.type.class, meta.return.type, class.type.name, storage.type.variable, meta.type.annotation, return-type, inherited-class, entity.other.inherited-class, declaration.entity.type, declaration.heritage.type, storage.type.cs, support.class, new.expr",
      "settings": {
        "foreground": "#2DA7C9",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Comment",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#81B073"
      }
    },
    {
      "name": "String",
      "scope": "string, punctuation.definition.string, keyword.operator.quantifier.regexp",
      "settings": {
        "foreground": "#EC9D9D"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#D9D900"
      }
    },
    {
      "name": "Property name",
      "scope": "property-name, support.function, meta.function-call",
      "settings": {
        "foreground": "#91D0C4"
      }
    },
    {
      "name": "Structure property name",
      "scope": "support.type.property-name, punctuation.support.type.property-name",
      "settings": {
        "foreground": "#D7BA7D"
      }
    },
    {
      "name": "Tag Name",
      "scope": "entity.name.tag, selector, selector.attribute-name",
      "settings": {
        "foreground": "#FFA346"
      }
    },
    {
      "name": "Tag Delimiter",
      "scope": "meta.tag.begin, meta.tag.end",
      "settings": {
        "foreground": "#30A2C2"
      }
    },
    {
      "name": "Tag Attribute Name",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#30C9AE"
      }
    },
    {
      "name": "Tag Attribute Value",
      "scope": "tag.quoted, string.quoted.double.html, punctuation.definition.string.begin.html, punctuation.definition.string.end.html",
      "settings": {
        "foreground": "#A3A3A3"
      }
    },
    {
      "name": "Hyperlink",
      "scope": "detected-link, string.detected-link",
      "settings": {
        "foreground": "#80C2E2"
      }
    },
    {
      "name": "Tag Name (TSX)",
      "scope": "entity.name.tag.tsx, entity.name.tag.jsx, entity.name.tag.js",
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Tag Delimiter (TSX)",
      "scope": "meta.tag.begin.tsx, meta.tag.end.tsx, meta.tag.begin.jsx, meta.tag.end.jsx, meta.tag.begin.js, meta.tag.end.js",
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "name": "Tag Attribute (TSX)",
      "scope": "entity.other.attribute-name.tsx, entity.other.attribute-name.js, entity.other.attribute-name.jsx",
      "settings": {
        "foreground": "#00CCFF"
      }
    },
    {
      "name": "JS Keyword",
      "scope": "js.storage.type",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF8811"
      }
    }
  ]
}