{
  "name": "Mocoding-D",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#E8E0C8",
    "badge.background": "#569CD6",
    "editorCursor.foreground": "#E8E0C8"
  },
  "tokenColors": [
     {
      "name": "Other Text",
      "scope": "text, source, scope, variable, meta.object, meta.tag, support.constant, meta.function-call.arguments, variable.parameter.function, support.type.object, support.variable, constant.language.import-export-all",
      "settings": {        
        "foreground": "#E8E0C8"
      }
    },
    {
      "name": "Keyword",
      "scope": "entity.name.tag, keyword.control, keyword.type, keyword.other, keyword.operator.new, keyword.operator.expression, constant.language, storage, primitive, modifier, language.constant, meta.preprocessor, variable.language.this, support.type.primitive, keyword.operator.logical.python, entity.name.type.terraform",
      "settings": {        
        "foreground": "#f28d29"
      }
    },    
    {
      "name": "Operator / Punctuation",
      "scope": "punctuation, keyword.operator, storage.type.function.arrow, meta.brace, keyword.other.interpolation",
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Type Parameters",
      "scope": "meta.type.parameters, entity.name.type.type-parameter, constant.character.format.placeholder",
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "entity.name.type, entity.name.type.alias, entity.name.type.class, entity.name.type.module, meta.return.type, class.type.name, storage.type.variable, meta.type.annotation, return-type, inherited-class, entity.other.inherited-class, declaration.entity.type, declaration.heritage.type, storage.type.cs, support.class, new.expr",
      "settings": {
        "foreground": "#2DA7C9",
        "fontStyle": "normal"
      }
    },
    {
      "name": "String",
      "scope": "string.regexp, string.quoted, string.template, punctuation.definition.string, keyword.operator.quantifier.regexp",
      "settings": {
        "foreground": "#eda4a4"
      }
    },
    {
        "name": "Comment",
        "scope": "comment, punctuation.definition.comment",
        "settings": {
          "foreground": "#81B073"
        }
      },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#D9D900"
      }
    },
    {
      "name": "Property name",
      "scope": "property-name, support.function, support.function.builtin, entity.name.function, variable.legacy.builtin",
      "settings": {
        "foreground": "#91D0C4"
      }
    },
    {
      "name": "Structure property name",
      "scope": "support.type.property-name, punctuation.support.type.property-name, entity.name.tag.yaml",
      "settings": {
        "foreground": "#D7BA7D"
      }
    },
    {
      "name": "Tag Name",
      "scope": "selector, selector.attribute-name",
      "settings": {
        "foreground": "#FFA346"
      }
    },
    {
      "name": "Tag Delimiter",
      "scope": "meta.tag.begin, meta.tag.end",
      "settings": {
        "foreground": "#30A2C2"
      }
    },
    {
      "name": "Tag Attribute Name",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#30C9AE"
      }
    },
    {
      "name": "Tag Attribute Value",
      "scope": "tag.quoted, string.quoted.double.html, punctuation.definition.string.begin.html, punctuation.definition.string.end.html, ",
      "settings": {
        "foreground": "#A3A3A3"
      }
    },
    {
      "name": "Hyperlink",
      "scope": "detected-link, string.detected-link",
      "settings": {
        "foreground": "#80C2E2"
      }
    },
    {
      "name": "Tag Name (TSX)",
      "scope": "entity.name.tag.tsx, entity.name.tag.jsx, entity.name.tag.js",
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Tag Delimiter (TSX)",
      "scope": "meta.tag.begin.tsx, meta.tag.end.tsx, meta.tag.begin.jsx, meta.tag.end.jsx, meta.tag.begin.js, meta.tag.end.js",
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "name": "Tag Attribute (TSX)",
      "scope": "entity.other.attribute-name.tsx, entity.other.attribute-name.js, entity.other.attribute-name.jsx",
      "settings": {
        "foreground": "#00CCFF"
      }
    },
    {
      "name": "JS Keyword",
      "scope": "js.storage.type",
      "settings": {        
        "foreground": "#f28d29"
      }
    },
    {
      "name": "Python DocString Comments",
      "scope": ["string.quoted.docstring.multi.python",
                "string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
                "string.quoted.docstring.multi.python punctuation.definition.string.end.python"],
      "settings": {
        "foreground": "#81B073",
        "fontStyle": "italic"
      }
    }
  ]
}